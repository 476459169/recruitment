.title {
  background-color: #f5f7f8;
  padding: 10px;
  font-size: 12px;
  height: 10px;
  line-height: 10px;
}
.cell .cell_content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.cell .cell_content .cell_content_name {
  font-size: 14px;
  color: #333333;
  margin: 10px;
}
.cell .cell_content .cell_content_name_o {
  color: #e8654b;
}
.cell .cell_content .cell_content_value {
  font-size: 12px;
  color: #666666;
  margin-right: 10px;
  padding: 5px;
  height: 25px;
  line-height: 25px;
}
.cell .cell_content .cell_downBtn {
  color: #CCCCCC;
  font-size: 12px;
  height: 25px;
  line-height: 25px;
  text-align: right;
  margin-right: 10px;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.cell .cell_content .cell_content_img {
  margin-left: 10px;
  margin-right: 10px;
  width: 20px;
  height: 20px;
}
.cell .cell_line {
  width: 100%;
  background-color: #f5f7f8;
  height: 1px;
}
.line {
  width: 100%;
  background-color: #f5f7f8;
  height: 1px;
}
.commitBtn {
  width: 80%;
  margin-left: 10%;
  margin-top: 50px;
  margin-bottom: 15px;
  height: 35px;
  line-height: 35px;
  font-size: 14px;
  color: #FFFFFF;
  background-color: #e8654b;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  text-align: center;
}

